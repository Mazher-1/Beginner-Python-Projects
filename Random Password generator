import random

uppercase_letters = "ABCDEFGHIJKLMNOPQRSTUVWXYZ"
lower_case_Letters= uppercase_letters.lower()
digits='01234560789'

upper, lower ,num =True,True,True
all =""

if upper :
    all+= uppercase_letters
if lower :
    all+=lower_case_Letters
if num :
    all+= digits

lenght = 8
amount= 10

for x in range(amount):
    password ="".join(random.sample(all,lenght))
    print(password)
